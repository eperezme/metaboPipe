@article{marco-ramellEvaluationComparisonBioinformatic2018,
  title = {Evaluation and Comparison of Bioinformatic Tools for the Enrichment Analysis of Metabolomics Data},
  author = {Marco-Ramell, Anna and Palau-Rodriguez, Magali and Alay, Ania and Tulipani, Sara and Urpi-Sarda, Mireia and Sanchez-Pla, Alex and Andres-Lacueva, Cristina},
  date = {2018-01-02},
