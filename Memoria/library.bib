@article{yuanDiscriminationMissingData2024,
  title = {Discrimination of Missing Data Types in Metabolomics Data Based on Particle Swarm Optimization Algorithm and {{XGBoost}} Model},
  author = {Yuan, Yang and Du, Jianqiang and Luo, Jigen and Zhu, Yanchen and Huang, Qiang and Zhang, Mengting},
  date = {2024-01-02},
  journaltitle = {Scientific Reports},
  shortjournal = {Sci Rep},
