  title = {Investigating Sources of Variability in Metabolomic Data in the {{EPIC}} Study: The {{Principal Component Partial R-square}} ({{PC-PR2}}) Method},
  shorttitle = {Investigating Sources of Variability in Metabolomic Data in the {{EPIC}} Study},
  author = {Fages, Anne and Ferrari, Pietro and Monni, Stefano and Dossus, Laure and Floegel, Anna and Mode, Nicolle and Johansson, Mattias and Travis, Ruth C. and Bamia, Christina and Sánchez-Pérez, María-José and Chiodini, Paolo and Boshuizen, Hendriek C. and Chadeau-Hyam, Marc and Riboli, Elio and Jenab, Mazda and Elena-Herrmann, Bénédicte},
