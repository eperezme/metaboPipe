@article{danziMetabolomicsTechnologicalPortfolio2023,
  title = {To Metabolomics and beyond: A Technological Portfolio to Investigate Cancer Metabolism},
  shorttitle = {To Metabolomics and Beyond},
  author = {Danzi, Federica and Pacchiana, Raffaella and Mafficini, Andrea and Scupoli, Maria T. and Scarpa, Aldo and Donadelli, Massimo and Fiore, Alessandra},
