  title = {{{OmicsSuite}}: A Customized and Pipelined Suite for Analysis and Visualization of Multi-Omics Big Data},
  shorttitle = {{{OmicsSuite}}},
  author = {Miao, Ben-Ben and Dong, Wei and Gu, Yi-Xin and Han, Zhao-Fang and Luo, Xuan and Ke, Cai-Huan and You, Wei-Wei},
