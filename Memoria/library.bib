  title = {Plasma Metabolomic Biomarkers of Mixed Nuts Exposure Inversely Correlate with Severity of Metabolic Syndrome},
  author = {Mora-Cubillos, Ximena and Tulipani, Sara and Garcia-Aloy, Mar and Bulló, Mònica and Tinahones, Francisco J and Andres-Lacueva, Cristina},
