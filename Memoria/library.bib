@article{yangDataPreprocessingStrategy2015,
  title = {A Data Preprocessing Strategy for Metabolomics to Reduce the Mask Effect in Data Analysis},
  author = {Yang, Jun and Zhao, Xinjie and Lu, Xin and Lin, Xiaohui and Xu, Guowang},
