@article{zhangSerumMetabolomicsNovel2012,
  title = {Serum Metabolomics as a Novel Diagnostic Approach for Disease: A Systematic Review},
  shorttitle = {Serum Metabolomics as a Novel Diagnostic Approach for Disease},
